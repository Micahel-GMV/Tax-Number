        <Label Content="Стать" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="116"/>
        <ComboBox x:Name="ComboBoxSex" HorizontalAlignment="Left" Margin="156,45,0,0" VerticalAlignment="Top" Width="100" SelectionChanged="ComboBox_SelectionChanged">
